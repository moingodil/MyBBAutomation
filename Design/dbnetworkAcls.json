  { 
	"Resources": {
	
	"PrivateNetworkAcl": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "VpcId": {
          "Ref": "VPC"
        }
      }
    },
	
	"NetworkAclEntryInboundDB": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": {
          "Ref": "PrivateNetworkAcl"
        },
        "RuleNumber": "100",
        "Protocol": "tcp",
        "RuleAction": "allow",
        "Egress": "false",
        "CidrBlock": {
          "Fn::FindInMap": [ "Subnets", "WebServerSubnetA", "CidrBlock" ]
        },
        "PortRange": { "From": "3306", "To": "3306" }
        }
		
      
    },
	
	"NetworkAclEntryInboundDB2": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": {
          "Ref": "PrivateNetworkAcl"
        },
        "RuleNumber": "200",
        "Protocol": "tcp",
        "RuleAction": "allow",
        "Egress": "false",
        "CidrBlock": {
          "Fn::FindInMap": [ "Subnets", "WebServerSubnetB", "CidrBlock" ]
        },
        "PortRange": { "From": "3306", "To": "3306" }
      }
    },
	
	"NetworkAclEntryOutboundPrivateA": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": {
          "Ref": "PrivateNetworkAcl"
        },
        "RuleNumber": "300",
        "Protocol": "tcp",
        "RuleAction": "allow",
        "Egress": "true",
        "CidrBlock": {
          "Fn::FindInMap": [ "Subnets", "WebServerSubnetA", "CidrBlock" ]
        },
        "PortRange": {
          "From": "1024",
          "To": "65535"
        }
      }
    },

	"NetworkAclEntryOutboundPrivateB": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": {
          "Ref": "PrivateNetworkAcl"
        },
        "RuleNumber": "300",
        "Protocol": "tcp",
        "RuleAction": "allow",
        "Egress": "true",
        "CidrBlock": {
          "Fn::FindInMap": [ "Subnets", "WebServerSubnetB", "CidrBlock" ]
        },
        "PortRange": {
          "From": "1024",
          "To": "65535"
        }
      }
    },
	
    "PrivateSubnetNetworkAclAssociationA": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "RDSSubnetA"
        },
        "NetworkAclId": {
          "Ref": "PrivateNetworkAcl"
        }
      }
    },
	
	"PrivateSubnetNetworkAclAssociationB": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "RDSSubnetB"
        },
        "NetworkAclId": {
          "Ref": "PrivateNetworkAcl"
        }
      }
    }
	
	}
	}